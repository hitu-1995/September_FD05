<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

  <bean id="add1" class="com.cjc.Address">
    <constructor-arg name="area" value="Akurdi"/>
    <constructor-arg name="city" value="Pune"/>
  </bean>

  <bean id="add2" class="com.cjc.Address">
    <constructor-arg name="area" value="Akurdi"/>
    <constructor-arg name="city" value="Pune"/>
  </bean>

  <bean id="stu" class="com.cjc.Student" autowire="byType">
     <constructor-arg name="rollno" value="101"/>
     <constructor-arg name="name" value="XYZ"/>
     
  </bean>
</beans>
